<?js
var data = obj;
var self = this;
?>
<hr>
<dt class="name" id="<?js= id ?>">

    <h4 id="<?js= id ?>"><?js= data.attribs + name + (data.signature ? (/>(external:[^<]+)</.test(data.signature) && this.find({longname: /external:\w+/.exec(data.signature)[0]})[0] ? `<span class="type-signature"> :<a href="${this.find({longname: /external:\w+/.exec(data.signature)[0]})[0].see[0].slice(7, -1)}">${/external:\w+/.exec(data.signature)[0]}</a></span>` : data.signature) : '') ?></h4>

    <?js if (data.summary) { ?>
    <p class="summary"><?js= summary ?></p>
    <?js } ?>
</dt>
<dd>
    <?js if (data && data.description) { ?>
    <div class="description">
        <?js= data.description ?>
    </div>
    <?js } ?>

    <?js if (data && data.type && data.type.names) {?>
        <h5>Type:</h5>
        <ul>
            <li>
                <?js= self.partial('type.tmpl', data.type.names) ?>
            </li>
        </ul>
    <?js } ?>

    <?js= this.partial('details.tmpl', data) ?>

    <?js if (data.examples && examples.length) { ?>
        <h5>Example<?js= examples.length > 1? 's':'' ?></h5>
        <?js= this.partial('examples.tmpl', examples) ?>
    <?js } ?>
</dd>
